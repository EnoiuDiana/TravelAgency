<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="450.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Register.LoginController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Please, log in." />
        <VBox layoutX="14.0" layoutY="79.0" prefHeight="69.0" prefWidth="100.0" spacing="30.0">
            <children>
                <Label lineSpacing="30.0" text="Email:" />
                <Label lineSpacing="30.0" text="Password: " />
            </children>
        </VBox>
        <VBox layoutX="142.0" layoutY="70.0" prefHeight="120.0" prefWidth="277.0" spacing="10.0">
            <children>
                <TextField fx:id="emailTextField" />
            <PasswordField fx:id="passwordTextField" />
            </children>
        </VBox>
        <Button layoutX="15.0" layoutY="184.0" mnemonicParsing="false" onAction="#loginButtonPushed" text="Log in" />
        <Label layoutX="14.0" layoutY="249.0" text="Don't have an account? " />
        <Button layoutX="15.0" layoutY="270.0" mnemonicParsing="false" onAction="#signupButtonPushed" text="Sign up" />
    </children>
</AnchorPane>
